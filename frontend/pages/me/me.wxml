<view class="page">
    <view class="page__hd">
        <view class="page__title">{{username}}的主页</view>
    </view>

    <view class="weui-cell" catchtap="test" id="ALL">
        <div class="weui-cell__bd">
            <p>所有项目</p>
        </div>
        <div class="weui-cell__ft">
            <view> > </view>
        </div>
    </view>

    <view class="weui-cell" catchtap="test" id="CHECKING">
        <div class="weui-cell__bd">
            <p>审核中的项目</p>
        </div>
        <div class="weui-cell__ft">
            <view> > </view>
        </div>
    </view>

    <view class="weui-cell" catchtap="test" id="CURRENT">
        <div class="weui-cell__bd">
            <p>正在进行的项目</p>
        </div>
        <div class="weui-cell__ft">
            <view> > </view>
        </div>
    </view>

    <view class="weui-cell" catchtap="test" id="HISTORY">
        <div class="weui-cell__bd">
            <p>历史项目</p>
        </div>
        <div class="weui-cell__ft">
            <view> > </view>
        </div>
    </view>
</view>