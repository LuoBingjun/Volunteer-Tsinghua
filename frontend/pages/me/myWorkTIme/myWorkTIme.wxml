<view class="page">
    <view class="page__hd">
        <view class="page__title">欢迎回来！{{username}}</view>
        <view class="page__desc">我的总工时：{{userTotalWorkTime}}</view>
    </view>
    <view class="page_bd">
        <ul >
                <view wx:for="{{projects}}" class="VBox projectBox">
                    <view style="background:url(\"{{item.imageUrl}}\");width:100%;height:100%;background-size: cover"
                     bindtap="onClickOneProject">
                        <view>{{item.name}}</view>
                        <view class="text_gray">描述：{{item.description}}</view>
                    </view>
                </view>
                <!--<view class ="cutline"></view>-->
        </ul>
    </view>
</view>